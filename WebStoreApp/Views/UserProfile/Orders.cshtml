@model IEnumerable<UserOrderViewModel>
@{
    Layout = "_UserProfileLayout";
    ViewData["Title"] = "Ваши заказы";
}


<table class="table">
    <thead>
        <tr>
            <th>№</th>
            <th>Имя</th>
            <th>Адрес</th>
            <th>Телефон</th>
            <th>Сумма</th>
        </tr>
    </thead>
    <tbody>
        @{ var i = 1;}
        @foreach (var order in Model)
        {
            <tr>
                <th>@(i++)</th>
                <th>@order.Name</th>
                <th>@order.Address</th>
                <th>@order.Phone</th>
                <th>@order.TotalSum</th>
            </tr>
        }
    </tbody>
</table>